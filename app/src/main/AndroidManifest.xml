<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kh.com.omarket">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.RegistrationActivity"
            android:label="@string/sign_up"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.LoginActivity"
            android:label="@string/sign_in"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.SellActivity1"
            android:label="@string/sell"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.SellActivity3"
            android:label="@string/sell"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.MyProductActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.ProductDetailActivity" />
        <activity
            android:name=".activities.FavoriteActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity android:name=".activities.test" />
        <activity android:name=".customGallery.activities.AlbumSelectActivity" />
        <activity android:name=".customGallery.activities.ImageSelectActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SellActivity1" />
    </application>

</manifest>